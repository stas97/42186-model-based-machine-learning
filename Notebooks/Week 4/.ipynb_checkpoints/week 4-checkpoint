   ],
   "source": [
    "simple_vector_STAN=\"\"\"\n",
    "data {\n",
    "    int N;\n",
    "    vector[N] a;\n",
    "}\n",
    "transformed data {\n",
    "    real b=mean(a);\n",
    "    print(\"b=\", b); // check this in the console!!!\n",
    "}\n",
    "parameters {\n",
    "    real x;\n",
    "    real y;\n",
    "}\n",
    "model {\n",
    "    x ~ normal(b, 10);\n",
    "    a ~ normal(y, 10);\n",
    "}\n",
    "\"\"\"\n",
    "\n",
    "# compile model\n",
    "sm = pystan.StanModel(model_code=simple_vector_STAN)"
   ]
